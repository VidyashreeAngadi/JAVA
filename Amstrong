import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int temp=n;
        int am=0;
		while(n>0)
		{
		   int digit=n%10;
		   n=n/10;
		   am=am+digit*digit*digit;
		}
		if(temp==am)
		{
		    System.out.println("Amstrong...");
		}
		else{
		    System.out.println("Not amstrong...");
		}
	}
}
