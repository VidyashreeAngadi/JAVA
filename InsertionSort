class Solution
{
  public void insertionSort(int arr[], int n)
  {
      //code here
      int temp=0;
      int i,j;
      for(i=1;i<n;i++)
      {
          temp=arr[i];
          j=i;
          while(j>0 && arr[j-1]>temp)
          {
              arr[j]=arr[j-1];
              j=j-1;
          }
          arr[j]=temp;
      }
  }
}
