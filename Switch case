class Solution{
    static double switchCase(int choice, List<Double> arr){
        // code here
        double PI=3.14159265;
        switch(choice)
        {
            case 1:
                double R=arr.get(0);
                return R*R*PI;
            case 2:
                double L=arr.get(0);
                double B=arr.get(1);
                return L*B;
        }
        return 0.0;
    }
}
