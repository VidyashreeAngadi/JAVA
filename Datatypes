class Solution {
    static int dataTypeSize(String str) {
        // code here
        if(str.equals("Character"))
           return 2;
           if(str.equals("Integer"))
           return 4;
           if(str.equals("Long"))
           return 8;
           if(str.equals("Float"))
           return 4;
           if(str.equals("Double"))
           return 8;
           else
           return -1;
    }
}
