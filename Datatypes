class Solution {
    static int dataTypeSize(String str) {
        // code here
        switch(str)
        {
            case "Character":
            case "char":
                return 1;
            case "Integer":
            case "int":
                return 4;
            case "Long":
                return 8;
            case "Float":
                return 4;
            case "Double":
                return 8;
            default :
            return -1;
        }
    }
}
